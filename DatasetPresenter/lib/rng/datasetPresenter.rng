<?xml version="1.0" ?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
         datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <element name="datasetPresenters">
      <zeroOrMore>
        <element name="datasetPresenter">
          <attribute name="name" />
          <optional>
	    <choice>
	      <attribute name="datasetNamePattern" />
	      <attribute name="override" />
	    </choice>
          </optional>
          <element name="displayName">
            <text/>
          </element>
	  <optional>
	    <element name="shortDisplayName">
	      <text/>
	    </element>
	  </optional>
	  <optional>
	    <element name="shortAttribution">
	      <text/>
	    </element>
	  </optional>
	  <optional>
	    <element name="categoryOverride">
	      <text/>
	    </element>
	  </optional>
          <element name="summary">
            <text/>
          </element>
          <element name="description">
            <text/>
          </element>
	  <optional>
	    <element name="usage">
	      <text/>
	    </element>
	  </optional>
	  <optional>
	    <element name="protocol">
	      <text/>
	    </element>
	  </optional>
	  <optional>
	    <element name="caveat">
	      <text/>
	    </element>
	  </optional>
	  <optional>
	    <element name="acknowledgement">
	      <text/>
	    </element>
	  </optional>
	  <optional>
	    <element name="releasePolicy">
	      <text/>
	    </element>
	  </optional>
          <zeroOrMore>
            <element name="history">
	      <attribute name="buildNumber">
		<data type="float"/>
	      </attribute>
	      <optional>
		<attribute name="genomeSource"/>
		<attribute name="genomeVersion"/> 
		<attribute name="annotationSource"/>
		<attribute name="annotationVersion"/> 
	      </optional>
	      <optional>
		<attribute name="functionalAnnotationSource"/>
		<attribute name="functionalAnnotationVersion"/> 
	      </optional>
	      <text/>
            </element>
          </zeroOrMore>
	  <element name="primaryContactId">
	    <text/>
	  </element>
	  <zeroOrMore>
	    <element name="contactId">
	      <text/>
	    </element>
	  </zeroOrMore>
	  <zeroOrMore>
	    <element name="link">
	      <optional>
		<attribute name="isPublication"/>
	      </optional>
              <optional>
                <element name="text">
                  <text/>
                </element>
              </optional>
              <optional>
                <element name="description">
                  <text/>
                </element>
              </optional>
	      <element name="url">
		<text/>
	      </element>
	    </element>
	  </zeroOrMore>
	  <zeroOrMore>
	    <element name="pubmedId">
	      <text/>
	    </element>
	  </zeroOrMore>
          <zeroOrMore>
            <element name="wdkReference">
	      <attribute name="name"/>
	      <attribute name="recordClass"/>
	      <attribute name="type"/>
            </element>
          </zeroOrMore>
          <zeroOrMore>
	    <element name="templateInjector">
	      <attribute name="className"/>
              <attribute name="projectName" />
              <optional>
	        <attribute name="datasourceName"/>
              </optional>
              <optional>
	        <attribute name="categoryOverride"/>
              </optional>
	      <zeroOrMore>
		<element name="prop">
		  <attribute name="name"/>
		  <text/>
		</element>
	      </zeroOrMore>
	    </element>
          </zeroOrMore>
        </element>
      </zeroOrMore>
      <zeroOrMore>
        <element name="internalDataset">
          <attribute name="name"/>
          <optional>
            <attribute name="datasetNamePattern" />
          </optional>
	</element>
      </zeroOrMore>
    </element>
  </start>
</grammar>
