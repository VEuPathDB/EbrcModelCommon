    <prop name="projectName">$$projectName$$</prop>
    <prop name="experimentName">LlinasMetabolites</prop>
    <prop name="experimentVersion">2012-12-12</prop>
    <prop name="doExpressionAnalysis">false</prop>
    <prop name="hasTimeSeries">false</prop> 
  </dataset>
-->

  <!-- substances and compounds based on metabolite data -->
