<datasets>
  <constant name="projectName" value="TrichDB"/>
  <constant name="genbankIsolateMapperVersion" value="2015-11"/>

  <dataset class="organism">
    <prop name="organismAbbrev">tvagG3</prop>
    <prop name="projectName">$$projectName$$</prop>
    <prop name="ncbiTaxonId">412133</prop>
    <prop name="publicOrganismAbbrev">tvagG3</prop>
    <prop name="organismFullName">Trichomonas vaginalis G3</prop>
    <prop name="organismNameForFiles">TvaginalisG3</prop>
    <prop name="speciesNcbiTaxonId">5722</prop>
    <prop name="isReferenceStrain">true</prop>
    <prop name="referenceStrainOrganismAbbrev">tvagG3</prop>
    <prop name="isAnnotatedGenome">true</prop>
    <prop name="hasTemporaryNcbiTaxonId">false</prop>
    <prop name="orthomclAbbrev">tvag</prop>
    <prop name="strainAbbrev">G3</prop>
    <prop name="genomeSource">JCarltonLab</prop>
    <prop name="taxonHierarchyForBlastxFilter">Eukaryota Trichomonadida Trichomonas</prop>
    <prop name="annotationIncludesTRNAs">true</prop>
    <prop name="runExportPred">false</prop>
    <prop name="maxIntronSize">1200</prop> 
    <prop name="isHugeGenome">false</prop>
    <prop name="genomeVersion">2014-09-12</prop>
    <prop name="isFamilyRepresentative">true</prop>
    <prop name="familyRepOrganismAbbrev">tvagG3</prop>
    <prop name="familyNcbiTaxonIds">181550</prop>
    <prop name="familyNameForFiles">Trichomonadidae</prop>
  </dataset>

  <dataset class="referenceStrain">
    <prop name="organismAbbrev">tvagG3</prop>
    <prop name="isAnnotatedGenome">true</prop>
  </dataset>

  <dataset class="orthomcl">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="version">5.17</prop>
  </dataset>

  <dataset class="orthomclPhyletic">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="version">5.17</prop>
  </dataset>

  <dataset class="orthomclTree">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="version">5.17</prop>
  </dataset>

  <!-- load of all KEGG pathways -->
  <dataset class="Pathways">
    <prop name="version">2013.07</prop>
    <prop name="format">KEGG</prop>
    <prop name="source">KEGG</prop>
    <prop name="projectName">$$projectName$$</prop>
  </dataset>

  <dataset class="Pathways">
    <prop name="version">2017.02.07</prop>
    <prop name="format">BioCyc</prop>
    <prop name="source">MetaCyc</prop>
    <prop name="projectName">$$projectName$$</prop>
  </dataset>

  <!-- load of all KEGG substances and compounds -->
<!--
  <dataset class="PathwayCompounds">
    <prop name="name">pubchem</prop>
    <prop name="version">2012.11</prop>
  </dataset>
-->

  <!-- profiles for metabolite data -->
