  <constant name="familyRepOrganismAbbrev" value="calbSC5314"/>
  <constant name="projectName" value="FungiDB"/>
  <constant name="ncbiTaxonId" value="237561"/>
  <constant name="speciesNcbiTaxonId" value="5476"/>
