    <prop name="version">2012-01-31</prop>
    <prop name="hasPairedEnds">true</prop>
    <prop name="isColorspace">false</prop>
    <prop name="snpPercentCutoff">20</prop>
  </dataset>

  <dataset class="copyNumberVariationExperiment">
