    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="experimentName">Tb_gambiense_GeorgeCross</prop>
    <prop name="experimentVersion">2012-01-31</prop>
    <prop name="ploidy">2</prop>
  </dataset>

